                var squareLegendX = (this['options']['chart']['type'] == 'line' ? 0 : 3);
                this.update(
                  {
                    legend: {
                      x: this.yAxis[0].left - this.spacingBox.x - this.legend.padding - squareLegendX
                    }
                  }
                );
              }              
          }
