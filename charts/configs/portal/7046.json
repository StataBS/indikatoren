                                    },
                                    text: ''
                                },
                                labels: {
                                    y: 3,
                                    style: {
                                        fontSize: '10px',
                                        color: "#000000"
                                    }
                                }
                            }]
                        });
                    }
                },
      "render": function(args){
        let chart = this;
          chart.xAxis[0].labelGroup.element.childNodes.forEach(label => {
            if(label.getBBox().height > 9){
            	label.attributes[4].value = label.attributes[4].value - (label.getBBox().height - 9)/4
            }
          })
 
        }
