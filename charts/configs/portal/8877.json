						txt = txt.replace("mögl.", "m.")
							.replace("im ", "")
							.replace("am ", "")
							.replace("in ", "")
							.replace("versorgung", "vers.")
							.replace("einrichtungen", "einricht.");

						txt = ((txt.replace(/\s/g, "") == "") ? txt : txt + ' (' + nString + sum + ')');

