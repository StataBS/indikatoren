          const chart = this,
            colors = ['#59fb59', '#fbf659', '#fb9999'],
            data = chart.series[0].data,
            assessed = chart.series[3].data;
          data.forEach(function (element, i) {
            if (assessed[i].y !== null) {
              element.update({
